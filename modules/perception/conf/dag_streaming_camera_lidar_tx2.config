# Define all nodes in DAG streaming.
subnode_config {
    # 64-Lidar Input nodes.
    subnodes {
        id: 1
        name: "Lidar16ProcessSubnode"
        reserve: "device_id:velodyne16;"
        type: SUBNODE_IN
    }

      # Camera node
    subnodes {
        id: 3
        name: "CameraProcessSubnode"
        reserve: "device_id:camera;pb_obj:0;pb_ln_msk_:1;"
        type: SUBNODE_IN
    }

      # Fusion node.
    subnodes {
        id: 31
        name: "FusionSubnode"
        reserve: "pub_driven_event_id:1001;lidar_event_id:1001;camera_event_id:1009"
        type: SUBNODE_OUT
    }
}

  ###################################################################
# Define all edges linked nodes.
edge_config {
    # Lidar16ProcessSubnode -> FusionSubnode
    edges {
        id: 101
        from_node: 1
        to_node: 31
        events {
            id: 1001
            name: "lidar_fusion"
        }
    }

      # CameraProcessSubnode -> FusionSubnode
    edges {
        id: 109
        from_node: 3
        to_node: 31
        events {
            id: 1009
            name: "camera_fusion"
        }
    }
}

  data_config {
    datas {
        id: 5
        name: "CameraObjectData"
