<!--this file list the modules which will be loaded dynamicly and
    their process name to be running in -->

<cyber>
    <desc>cyber modules list config</desc>
    <version>1.0.0</version>

    <!-- sample module config, and the files should have relative path like
         ./bin/cyber_launch
         ./bin/mainboard
         ./conf/dag_streaming_0.conf -->

    <module>
        <name>perception_camera</name>
        <dag_conf>/apollo/modules/perception/dag/camera_detection.dag</dag_conf>
        <!-- if not set, use default process -->
        <process_name>perception_camera</process_name>
        <version>1.0.0</version>
    </module>

    <module>
        <name>motion</name>
        <dag_conf>/apollo/modules/perception/dag/motion.dag</dag_conf>
        <!-- if not set, use default process -->
        <process_name>motion</process_name>
        <version>1.0.0</version>
    </module>
</cyber>
